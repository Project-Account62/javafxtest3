<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="300.0" maxWidth="250.0" minHeight="300.0" minWidth="250.0" prefHeight="300.0" prefWidth="250.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxtest3.controller.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TextField maxHeight="0.0" maxWidth="0.0" opacity="0.0" />
   <TextField fx:id="numLift" promptText="Number of Lifts" />
   <TextField fx:id="numFloor" promptText="Number of Floors" />
   <TextField fx:id="movingTime" promptText="Moving Time (Default 5)" />
   <TextField fx:id="waitingTime" promptText="Waiting Time (Default 3)" />
  <Button fx:id="button" onAction="#onGoButtonClick" text="Go!" />
   <Label fx:id="label" />
</VBox>
